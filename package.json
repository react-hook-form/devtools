{
  "name": "@hookform/devtools",
  "version": "1.2.1",
  "description": "React Hook Form dev tool to help debugging forms",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/react-hook-form/devtools.git"
  },
  "author": "bluebill1049 <bluebill1049@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "prepublishOnly": "yarn lint && yarn test && yarn run clean && yarn build",
    "test": "jest --runInBand",
    "lint": "yarn lint:check --fix && yarn prettier",
    "prettier": "prettier --write './src/**/*.ts' './src/**/*.tsx' --config ./.prettierrc",
    "lint:check": "eslint ./src --ext .jsx,.ts --ignore-pattern *.test.ts --report-unused-disable-directives",
    "test:watch": "yarn test -- --watchAll --coverage"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^5.6.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@types/lodash": "^4.14.149",
    "little-state-machine": "^3.0.0",
    "lodash": "^4.17.15",
    "react-simple-animate": "^3.3.6"
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^5.6.1",
    "rollup": "^1.31.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "bugs": {
    "url": "https://github.com/react-hook-form/devtools/issues"
  },
  "homepage": "https://github.com/react-hook-form/devtools#readme"
}
