  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "prepublishOnly": "yarn lint && yarn test && yarn run clean && yarn build",
    "test": "jest --runInBand",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:types": "tsc --noEmit",
    "lint:check": "eslint ./src --ext .jsx,.ts --ignore-pattern *.test.ts --report-unused-disable-directives",
    "test:watch": "yarn test -- --watchAll --coverage"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-hook-form": ">=6.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/lodash": "^4.14.152",
    "little-state-machine": "^3.0.1",
    "lodash": "^4.17.15",
    "react-simple-animate": "^3.3.8"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "rollup": "^2.10.7",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "bugs": {
    "url": "https://github.com/react-hook-form/devtools/issues"
  },
  "homepage": "https://github.com/react-hook-form/devtools#readme"
